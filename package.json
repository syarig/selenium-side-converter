{
  "name": "selenium-side-converter",
  "description": "Command line tool for converting template in selenium .side file. ",
  "version": "1.3.1",
  "author": "syarig",
  "bin": {
    "selenium-side-converter": "./bin/selenium-side-converter.js"
  },
  "bugs": {
    "url": "https://github.com/syarig/selenium-side-converter/issues",
    "email": "syarig110@yahoo.co.jp"
  },
  "dependencies": {
    "commander": "^9.0.0",
    "lodash": "^4.17.15",
    "log4js": "^6.1.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@types/jest": "27.4.1",
    "@types/lodash": "4.14.180",
    "@types/log4js": "2.3.5",
    "@types/node": "16.11.26",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "eslint": "8.12.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.7",
    "semantic-release": "19.0.2",
    "ts-jest": "27.1.4",
    "ts-loader": "9.2.8",
    "tsconfig-paths": "3.14.1",
    "typescript": "4.6.3",
    "webpack": "5.70.0",
    "webpack-cli": "4.9.2",
    "webpack-merge": "5.8.0"
  },
  "eslintIgnore": [
    "/jest.config.js",
    "/webpack.config.js",
    "/webpack.dev.js",
    "/webpack.prod.js"
  ],
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "bin",
    "dist"
  ],
  "homepage": "https://github.com/syarig/selenium-side-converter",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "CLI",
    "selenium"
  ],
  "license": "Apache-2.0",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "main": "index.js",
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/github",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/syarig/selenium-side-converter"
  },
  "scripts": {
    "build": "webpack --config webpack.dev.js",
    "lint": "eslint --ext .js --ignore-path .gitignore .",
    "prepare": "webpack --config webpack.prod.js",
    "test": "jest",
    "watch": "npm run build -- --watch",
    "semantic-release": "semantic-release"
  }
}
