{
  "name": "selenium-side-converter",
  "description": "Command line tool for converting template in selenium .side file. ",
  "version": "1.2.0",
  "author": "syarig",
  "bin": {
    "selenium-side-converter": "./bin/selenium-side-converter.js"
  },
  "bugs": {
    "url": "https://github.com/syarig/selenium-side-converter/issues",
    "email": "syarig110@yahoo.co.jp"
  },
  "dependencies": {
    "commander": "^4.0.1",
    "lodash": "^4.17.15",
    "log4js": "^6.1.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@types/jest": "26.0.14",
    "@types/lodash": "4.14.162",
    "@types/log4js": "2.3.5",
    "@types/node": "12.12.68",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "eslint": "7.11.0",
    "husky": "4.3.0",
    "jest": "26.5.3",
    "lint-staged": "10.4.2",
    "semantic-release": "17.2.1",
    "ts-jest": "26.4.1",
    "ts-loader": "8.0.5",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.0.3",
    "webpack": "4.44.2",
    "webpack-cli": "4.1.0",
    "webpack-merge": "5.2.0"
  },
  "eslintIgnore": [
    "/jest.config.js",
    "/webpack.config.js",
    "/webpack.dev.js",
    "/webpack.prod.js"
  ],
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "bin",
    "dist"
  ],
  "homepage": "https://github.com/syarig/selenium-side-converter",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "CLI",
    "selenium"
  ],
  "license": "Apache-2.0",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "main": "index.js",
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/github",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/syarig/selenium-side-converter"
  },
  "scripts": {
    "build": "webpack --config webpack.dev.js",
    "lint": "eslint --ext .js --ignore-path .gitignore .",
    "prepare": "webpack --config webpack.prod.js",
    "test": "jest",
    "watch": "npm run build -- --watch",
    "semantic-release": "semantic-release"
  }
}
